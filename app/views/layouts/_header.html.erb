<header>
  <h1>Header Image here</h1>
  <nav>
    <ul>
      <% if current_user  %>
        <li>Logged in as <%= current_user.email %></li>
        <li><%= link_to "Log out", log_out_path %></li>
      <% else  %>
        <li><%= link_to "Sign up", sign_up_path %> or 
	<%= link_to "Log in", log_in_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
